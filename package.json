  "description": "Group work for course COMP.SE.200",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "test-with-coverage": "nyc --reporter=html --reporter=text mocha test --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": true,
    "include": ["src/*.js"],
