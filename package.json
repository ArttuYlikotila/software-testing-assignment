                "src/countBy.js", "src/defaultTo.js", "src/defaultToAny.js",
                "src/difference.js", "src/divide.js", "src/drop.js",
                "src/endsWith.js", "src/every.js", "src/filter.js",
                "src/isArrayLikeObject.js", "src/isBoolean.js", 
                "src/isDate.js", "src/keys.js", "src/map.js", "src/memoize.js",
                "src/slice.js", "src/words.js"],
    "extension": [".js"]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  }
